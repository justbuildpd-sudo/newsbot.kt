{
  "name": "insightforge-2025",
  "rewrites": [
    {
      "source": "/app.js",
      "destination": "/frontend/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
